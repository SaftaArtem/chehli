}




function topHidePartOfMenu() {

    if ( $('.top_header_wrap  .top-menu').length ) {
        $('.more_li li').not('.more_li__first').hide();

        var menuWidth = $('.top_header_wrap  .top-menu').outerWidth(true);
        // console.log(menuWidth);

        var elementsMenuWidth = 0;

        $('.top_header_wrap  .top-menu li').each(function() {

            elementsMenuWidth = elementsMenuWidth + $(this).outerWidth();
            // console.log('item', elementsMenuWidth);

            if( menuWidth <= (elementsMenuWidth + 107) ) {

                $(this).css('display', 'none');
                var that = $(this).index();

                // console.log(that);



                $('.more_li li').each(function () {

                    if( ($(this).index() - 1) === that) {
                        // console.log($(this));
                        $(this).show();
                    }
                })
                // console.log($(this).index());


            } else {

                $(this).css('display', 'block');
            }
        });


        if( menuWidth <= (elementsMenuWidth + 107) ) {
            $('.more_li').addClass('show');
        } else {
            $('.more_li').removeClass('show');
        }



    }

}




function bodyClick() {
    $(document).unbind('click').click(function (e) {
        if( !$('.more_li').hasClass('hide_li') && !$(e.target).is('.more_li li') ) {
            $('.more_li').addClass('hide_li');
        }
    })
}


$(function () {
    
    function dvs_initCatalogLeftMenu(){
        var win = $(this);
        if (win.width() <= 992){
           $('.lc-has-child').off('click').html('Выберете категорию');
           $('.lc-has-child').on('click', function (e) {
                event.preventDefault();
                console.log($(this).siblings('.second_ul'));
                $(this).siblings('.second_ul').toggle();
            }); 
        }else{
            $('.lc-has-child').off('click');
            $('.second_ul').show();
        }
        
    };
    dvs_initCatalogLeftMenu();
    
    $(window).on('resize', function(){
        dvs_initCatalogLeftMenu();
    });
});




$(function () {
    
    function dvs_transferObject(target,targetTo,targetFrom){   
        var win = $(this);
        if (win.width() <= 992){
           $(target).detach().appendTo(targetTo);
        }else{
            $(target).detach().appendTo(targetFrom);
        }
    };
    init_dvsTransfer();
    
    $(window).on('resize', function(){
        init_dvsTransfer();
    });
    
    function init_dvsTransfer(){
        dvs_transferObject('.top-login','.top_header_wrap.cl','.top_header-menu');
        dvs_transferObject('.dvs_inner_element_article','.maxw815 h1','.maxw815');
        dvs_transferObject('.sing_in','.top_header_wrap.cl','.top_header-menu');
    };
});



$(document).ready(function () {  
    $('.dvs-menu-wrap').hide(); 
    $('.menu-sub').hide(); 
    $('.has_childrens').on('click', function () {   
        var name_id = $(this).attr("href");
        $('.has_childrens').removeClass("active");
        $(this).addClass("active");
        console.log(name_id);
    
        $('.menu-sub').hide(); 
        $(name_id).fadeIn(300);
        $('.dvs-menu-wrap').fadeIn(300);
    });
    
    $(document).mouseup(function (e) {
        var container = $('.dvs-menu-wrap');
        if (container.has(e.target).length === 0){
            container.hide();
        }
    });
    
    $('.close-sub').on('click', function (e) {
        e.preventDefault();
        $('.menu-sub').fadeOut(300);
        $('.dvs-menu-wrap').fadeOut(300);
    });
    
});

$(document).ready(function () {
	$('.jq-search_menu').on('input', function () {
	    var	val =  $(this).val().toLowerCase();
	    	ul = $(this).siblings(".jq-ul");
	    $(ul).find('a').each(function() {
		  var a = $(this).html();
		  if ($(this).html().toLowerCase().indexOf(val) != -1)
		  {
		  	$(this).parent().show();
		  }
		  else
		  {
		  	$(this).parent().hide();
		  }
		});
		
	});
});



