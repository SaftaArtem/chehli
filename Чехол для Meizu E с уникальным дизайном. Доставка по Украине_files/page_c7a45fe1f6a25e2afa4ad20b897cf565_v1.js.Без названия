$(function () {
    
    function dvs_moveCatalogTitle(){
        var win = $(this);
        if (win.width() <= 992){
           $('section.h1').detach().prependTo('.ready_shop.container-fluid');
        }else{
            $('section.h1').detach().prependTo('.clearfix.ajax_section');
        }
        
    };
    dvs_moveCatalogTitle();
    
    $(window).on('resize', function(){
        dvs_moveCatalogTitle();
    });
});



/* End */
